import java.util.*;

public class bilangan
{
	public static void main (String [] args)
	{
		Scanner sc = new Scanner (System.in);
		System.out.print("jumlah bilangan ganjil : ");
		int n=sc.nextInt();
		int i = 1;
		int bilganjil = 1;
		while (i<=n)
		{
			System.out.println(bilganjil);
			bilganjil +=2;
			i++;
		}
	}
}
